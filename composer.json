{
    "name": "jonneyless/yii2-app-cms",
    "description": "The Content Manage System Project Based Yii Framework",
    "keywords": [
        "yii2",
        "framework",
        "cms",
        "project template"
    ],
    "homepage": "http://www.ijony.com/",
    "language": "PHP",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "jonneyless",
            "email": "jonneyless@163.com",
            "homepage": "http://www.ijony.com",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/jonneyless/yii2-app-cms/issues",
        "wiki": "https://github.com/jonneyless/yii2-app-cms/wiki",
        "source": "https://github.com/jonneyless/yii2-app-cms"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": ">=2.0.6",
        "yiisoft/yii2-jui": "*",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-swiftmailer": "*",
        "yiisoft/yii2-imagine": "*",
        "2amigos/yii2-qrcode-helper": "*",
        "cdcchen/yii2-alidayu": "*",
        "moonlandsoft/yii2-phpexcel": "*"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*"
    },
    "config": {
        "process-timeout": 1800
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    },
    "scripts": {
        "post-install-cmd": "php init --env=Development --overwrite=n"
    }
}
